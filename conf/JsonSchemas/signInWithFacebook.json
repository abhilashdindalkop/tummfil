{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "SignIn With Facebook",
    "type": "object",
    "properties": {
        "facebookId": {
            "type": "string"
        },
        "fbToken": {
            "type": "string",
            "minLength": 3
            
        },
        "deviceId": {
            "type": "string",
            "maxLength": 100
        },
        "deviceToken": {
            "type": "string",
            "minLength": 3,
            "maxLength": 100
        },
        "deviceTypeId": {
            "type": "integer",
            "description": "Device type id for device os identification"
        }
    },
    "additionalProperties": false,
    "required": [
        "facebookId",
        "fbToken",
        "deviceTypeId"
    ]
}