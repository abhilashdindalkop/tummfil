{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "signUp",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"maxLength": 45,
			"minLength": 2,
			"description": "name"
		},
		"address": {
			"type": "string",
			"maxLength": 256
		},
		"pincode": {
			"type": "string",
			"maxLength": 20,
			"description": "pincode"
		},
		"cityId": {
			"type": "integer"
		},
		"phoneNo": {
			"type": "string",
			"maxLength": 25,
			"minLength": 10,
			"description": "PhoneNo"
		},
		"email": {
			"description": "emailId  is mandatory",
			"type": "string",
			"format": "email",
			"maxLength": 100
		},
		"password": {
			"description": "password",
			"type": "string",
			"maxLength": 100,
			"minLength": 6
		},
		"deviceToken": {
			"type": "string",
			"maxLength": 256,
			"description": "deviceToken for push notification"
		},
		"deviceId": {
			"type": "string",
			"maxLength": 256,
			"description": "Device vendor specific id"
		},
		"deviceType": {
			"type": "integer",
			"description": "Device type id for device OS identification"
		},
		"referralCode": {
			"type": "string",
			"maxLength": 25,
			"description": "referralCode"
		}
	},
	"additionalProperties": false,
	"required": [
		"name",
		"phoneNo",
		"password",
		"deviceId",
		"deviceType"
	]
}